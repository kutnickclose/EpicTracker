<li class="storyShow" data-rank="<%= story.get('rank') %>" data-id="<%= story.get('id') %>" data-list-id="<%= story.get('list_id') %>"><span class="storyBlock"><span class="edit">&#x25B8</span>
<% if (story.get('story_type') === "feature") { %>
	<span class="glyphicon glyphicon-star"></span>
<% } else if (story.get('story_type') === "bug" ) { %>
	<span class="glyphicon glyphicon-ban-circle"></span>
<% } else { %>
	<span class="glyphicon glyphicon-cog"></span>
<% } %>

<% if (story.get('points') === "0") { %>
	<button class="btn btn-primary btn-xs disabled"><%='0'%></button>
<% } else if (story.get('points') === "1" ) { %>
	<button class="btn btn-primary btn-xs disabled"><%='1'%></button>
<% } else if (story.get('points') === "2" ) { %>
	<button class="btn btn-primary btn-xs disabled"><%='2'%></button>
<% } else if (story.get('points') === "3" ) { %>
	<button class="btn btn-primary btn-xs disabled"><%='3'%></button>
<% } else { %>
	<button class="btn btn-default btn-xs disabled"><%='&nbsp; '%></button>
<% } %>
	 
<span class="storyText"><%= story.escape('title') %></span>


<div class="pull-right">
<% if (story.get('points') === "unestimated") { %>
	<button class="setZero btn btn-primary btn-xs"><%='0'%></button>
	<button class="setOne btn btn-primary btn-xs"><%='1'%></button>
	<button class="setTwo btn btn-primary btn-xs"><%='2'%></button>
	<button class="setThree btn btn-primary btn-xs"><%='3'%></button>
<% } else { %>
	<% if (story.get('state') === "unscheduled") { %>
		<%= '' %>
	<% } else if (story.get('state') === "unstarted" ) { %>
		<button class="start btn btn-primary">start</button>
	<% } else if (story.get('state') === "started" ) { %>
		<button class="finish btn btn-warning">finish</button>
	<% } else if (story.get('state') === "finished" ) { %>
		<button class="deliver btn btn-warning">deliver</button>
	<% } else if (story.get('state') === "delivered" ) { %>
		<button class="accept btn btn-success">accept</button> <button class="reject btn btn-danger">reject</button>
	<% } else if (story.get('state') === "rejected" ) { %>
		<button class="restart btn">restart</button>
	<% } else { %>
		<%= '' %>
	<% } %>
<% } %>
</div>

</span></li>